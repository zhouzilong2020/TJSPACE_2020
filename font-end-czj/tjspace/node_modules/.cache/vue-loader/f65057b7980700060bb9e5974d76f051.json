   userId=this.userInfo.userid,\n            nickname=this.Nickname,\n            gender=this.Gender,\n            phoneNumber=this.phoneNumber,\n            majorId=this.major,\n            year=this.Grade,\n            degree=this.degree,\n        }\n    }\n    },\n    methods:{\n      submit(){\n        alert(\"SelfInformation modified!\")  \n      }\n    },\n    created:{\n      async ModifyInfo() {\n        await this.$store.dispatch(\"InfoModify/infoModify\", this.information);\n        console.log(\"information modified!\");\n      }\n    },\n    }\n\n\n\n  // async AddOrModifyIt() {\n  //       await this.$axios.put(\"http://175.24.115.240:8080/api/Modify/Info\", {\n  //           userId=this.userInfo.userid,\n  //           nickname=this.Nickname,\n  //           gender=this.Gender,\n  //           phoneNumber=this.phoneNumber,\n  //           majorId=this.major,\n  //           year=this.Grade,\n  //           degree=this.degree,\n  //       })\n  //   },\n\n}\n\n</script>\n\n\n<style >\n.text-subtitle2{\n  margin-top: 40pt;\n}\n\n</style>"]}]}