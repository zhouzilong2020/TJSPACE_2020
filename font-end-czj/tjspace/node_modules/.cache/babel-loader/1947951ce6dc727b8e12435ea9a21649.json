) {\n      await this.$store.dispatch(\"userInfo/loginUser\", this.account);\n\n      if (this.token) {\n        // 成功获取token 表示成功登录\n        // console.log(\"get user token\")\n        console.log(this.userInfo)\n        this.$router.push({\n          name: \"Homepage\",\n          params: {\n            userId: this.userInfo.userid,\n          },\n        });\n      }\n    },\n  },\n};\n"]}]}